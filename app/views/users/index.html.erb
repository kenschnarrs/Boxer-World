<head>
    <title> Settings</title>
  
  </head>
<h1> Settings </h1>
<strong>  You can Change your Settings Here </strong> 

<table class="table table-hover">
  <thead class="table-dark">
    <tr>
      <th>Name</th>
      <th>Profile</th>
      <th>Settings</th>
      <th>Billing</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name%></td>
        <td><%= link_to 'Profile', user_path(user) %></td>
        <td><%= link_to 'Settings', edit_user_path(user)%></td>
        <td><%= link_to "Billing", billing_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>



