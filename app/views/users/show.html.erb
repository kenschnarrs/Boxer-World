  <!-- Calvin Gott, Dropdown menu, takes a user to show page, and then allows them to update username and blurd-->
<head>
    <title>User Customization</title>
  
  </head>
<h1>Welcome <%=@user.username%></h1> 
<%= bootstrap_form_with model: @user, url: user_path(@user), method: :patch, data: { turbo: false } do |f| %>
  <%= f.text_field :username%>
  <%= f.text_field :email %>
  <%= f.text_field :phone %>
  <%= f.text_field :address%>
  <%= f.submit %>
<% end %>








